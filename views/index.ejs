<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="stylesheet" href="index.css" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
      crossorigin="anonymous"
    ></script>

    <script
      src="https://kit.fontawesome.com/47aea1224e.js"
      crossorigin="anonymous"
    ></script>

    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.min.js"
      integrity="sha384-Rx+T1VzGupg4BHQYs2gCW9It+akI2MM/mndMCy36UVfodzcJcF0GGLxZIzObiEfa"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300&family=Oswald:wght@200&display=swap"
      rel="stylesheet"
    />
    <link rel="stylesheet" type="text/css" href="index.css" />
    <link
      rel="icon"
      type="icon"
      href="data:image/png;base64,"
    />
    <title>Hodlinfo</title>
  </head>
  <body>
    <div id="body" class="container-fluid">
      <div class="nav-container">
        <div class="logo-container text-center mt-3">
          <a href="#"
            ><img
              class="logo"
              src="https://hodlinfo.com/static/media/HODLINFO.8f78fc06.png"
              alt=""
          /></a>
        </div>
        <div class="button-container text-center mt-3">
          <select class="navgation-btn">
            <option>INR</option>
          </select>
          <select class="navgation-btn">
            <option>BTC</option>
            <option>ETH</option>
            <option>USDT</option>
            <option>XRP</option>
            <option>TRX</option>
            <option>DASH</option>
            <option>ZTC</option>
            <option>XEM</option>
            <option>LOST</option>
          </select>
         <a href="https://wazirx.com/invite/sp7pvbt6?utm_source=finstreet&utm_medium=affiliate&utm_campaign=regnow-btn"> 
          <button class="navgation-btn">BUY BTC</button>
          </a>
        </div>

        <div class="telegram-container mt-4">
          <p id="counter" class="counter">0</p>
         <a style="text-decoration: none;" href="https://hodlinfo.com/connect/telegram"><button class="telegram-btn">
            <img
              class="telegram-img"
              src="data:image/png;base64,"
              alt=""
            />
            Connect Telegram
          </button></a>
          <div id="toggle_container" class="toggle-container mt-3">
            <div id="toggle-btn" class="mini-toggle-container"></div>
          </div>
        </div>
      </div>

      <div class="text-container">
        <div class="text1">
          <p class="color1">0.28 %</p>
          <p class="color2">5 Mins</p>
        </div>
        <div class="text1">
          <p class="color1">0.63 %</p>
          <p class="color2">1 Hour</p>
        </div>

        <div class="text2">
          <p class="diff-color-1">Best Price To Trade</p>
          <h1 id="amount-text" class="diff-color-2">â‚¹ 23,77,314</h1>
          <p class="diff-color-3">
            Average BTC/INR net price including commision
          </p>
        </div>
        <div class="text1">
          <p class="color1">2.05 %</p>
          <p class="color2">1 Day</p>
        </div>
        <div class="text1">
          <p class="color1">6.61 %</p>
          <p class="color2">7 Days</p>
        </div>
      </div>
    
       <table>
        <thead>
          <tr>
            <th>
              <h4>#</h4>
            </th>
            <th>
              <h4>Platform</h4>
            </th>
            <th>
              <h4>Last Traded Price</h4>
            </th>
            <th>
              <h4>Buy / Sell Price</h4>
            </th>
            <th>
              <h4> Difference</h4>
            </th>
            <th>
              <h4>Savings</h4>
            </th>
          </tr>
        </thead>
      <tbody>
        <% users.forEach((crypto,index) => { %>
        <tr>
          <td>
            <h4 style="color: white;"><%= index + 1 %></h4>
          </td>
          <td>
            <h4 style="color: white;"><%=crypto.name %></h4>
          </td>
          <td>
            <h4 style="color: white;"><%=crypto.last %></h4>
          </td>
          <td>
            <h4 style="color: white;"><%=crypto.buy %> / <%=crypto.sell%></h4>
          </td>
          <td>
            <h4 style="color: white;"><%=crypto.volume %></h4>
          </td>
          <td>
            <h4 style="color: white;"><%=crypto.last %></h4>
          </td>
        </tr>
        <% }); %>
      </tbody>
       </table>
   
    </div>
  </body>

  <script>
    const ul = document.getElementById("ul");
     let toggle_btn = document.getElementById("toggle-btn");

    let body = document.getElementById("body");

    let toggle_container = document.getElementById("toggle_container");

    let navgation_btn = document.getElementsByClassName("navgation-btn");

    let amount_text = document.getElementById("amount-text");

    let main = true;

    let main_h1 = document.getElementsByTagName("h1");

    let main_ul = document.getElementById("ul");

    let counter = document.getElementById("counter");

    toggle_btn.addEventListener("click", () => {
      if (main) {
        body.style.backgroundColor = "white";
        body.style.color = "#0c0f48";
        toggle_container.style.justifyContent = "flex-start";
        toggle_container.style.backgroundColor = "white";
        amount_text.style.color = "#0c0f48";
        navgation_btn[0].style.backgroundColor = "white";
        navgation_btn[0].style.color = "#191d28";
        navgation_btn[1].style.backgroundColor = "white";
        navgation_btn[1].style.color = "#191d28";
        navgation_btn[2].style.backgroundColor = "white";
        navgation_btn[2].style.color = "#191d28";
        main_ul.classList.add("main-ul-class");

        main = false;
      } else {
        body.style.backgroundColor = "#191d28";
        toggle_container.style.justifyContent = "flex-end";
        amount_text.style.color = "white";
        toggle_container.style.backgroundColor = "#2e3241";
        navgation_btn[0].style.backgroundColor = "#2e3241";
        navgation_btn[0].style.color = "white";
        navgation_btn[1].style.backgroundColor = "#2e3241";
        navgation_btn[1].style.color = "white";
        navgation_btn[2].style.backgroundColor = "#2e3241";
        navgation_btn[2].style.color = "white";
        main_ul.classList.remove("main-ul-class");
        main = true;
      }
    });
setInterval(() => {
     let result = parseInt(counter.textContent) + 1; 
    counter.textContent = result;
    if (result == 60) {
        counter.textContent = 0;
    }
}, 1000);
  </script>
</html>
